# Application Configuration
VITE_APP_NAME=Portfolio Management System
VITE_APP_VERSION=1.0.0
VITE_NODE_ENV=staging

# API Configuration
VITE_API_BASE_URL=https://api-staging.yourdomain.com/api/v1
VITE_API_TIMEOUT=30000

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_SENTRY=true

# Authentication
VITE_JWT_REFRESH_TOKEN_KEY=refresh_token
VITE_JWT_ACCESS_TOKEN_KEY=access_token

# Logging
VITE_LOG_LEVEL=info

# External Services
VITE_SENTRY_DSN=https://your-sentry-dsn-staging@sentry.io/project-id
VITE_ANALYTICS_KEY=your-analytics-key-staging